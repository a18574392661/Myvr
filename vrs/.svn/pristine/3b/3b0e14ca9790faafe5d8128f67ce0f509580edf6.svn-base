public class RomeTypeDO implements Serializable {
	private static final long serialVersionUID = 1L;
	
	//
	private Integer romeId;
	private Integer topStyleId;//大风格id
	//小风格id
	private Integer leftStyleId;
	//房间名称
	private String romeName;
	//排序
	private Integer romeSort;
	
	private String topName;//大风格名称
	
	private String leftName;//小风格名称
	
	private String imgs;//热点图,用逗号隔开的
	
	private List<ColorTypeDO> color = new ArrayList<ColorTypeDO>();

	/**
	 * 设置：
	 */
	public void setRomeId(Integer romeId) {
		this.romeId = romeId;
	}
	/**
	 * 获取：
	 */
	public Integer getRomeId() {
		return romeId;
	}
	/**
	 * 设置：小风格id
	 */
	public void setLeftStyleId(Integer leftStyleId) {
		this.leftStyleId = leftStyleId;
	}
	/**
	 * 获取：小风格id
	 */
	public Integer getLeftStyleId() {
		return leftStyleId;
	}
	/**
	 * 设置：房间名称
	 */
	public void setRomeName(String romeName) {
		this.romeName = romeName;
	}
	/**
	 * 获取：房间名称
	 */
	public String getRomeName() {
		return romeName;
	}
	/**
	 * 设置：排序
	 */
	public void setRomeSort(Integer romeSort) {
		this.romeSort = romeSort;
	}
	/**
	 * 获取：排序
	 */
	public Integer getRomeSort() {
		return romeSort;
	}
