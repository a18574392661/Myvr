	<script>
		function to_price(index) {
			var b=$("#c"+index).prop("checked")
			$("#c"+index).prop("checked",!b);
			var ps=$("#p"+index).val();

			$("#ck").val($("#c"+index).val());

			$("#ps").html("￥"+ps);

		}

		function sta(dx) {

			$("#sta").val(dx.value);
		}

	</script>
   
   <script type="text/javascript">
   
	   	function animatefn(obj,flag){
	   		if(flag == 1){
	   			var id = $(obj).attr("id");
	   			$("#"+id).animate({"top":"-15px"},250);
	   		}else{
	   			var id = $(obj).attr("id");
	   			$("#"+id).animate({"top":"0px"},250);
	   		}
	   	}
	   	
	   	
	   	function msgfnl(obj,flag){
		   	  if(flag == 1){
		   		$("#msgl").fadeIn(800);
		   	  }else{
		   		$("#msgl").fadeOut(800);
		   	  }
	   	}
	   	
		function msgfnr(obj,flag){
		   	  if(flag == 1){
		   		$("#msgr").fadeIn(800);
		   	  }else{
		   		$("#msgr").fadeOut(800);
		   	  }
	   	}


	   	function to_pay() {
           //获得选中的开通时长的id


			$.get("/vrs/user/getUserVerity",function (data) {
				if(data.success==false){
					alert(data.message);
					location.href="/vrs/user/login?url=/pro/h5/queryVip";
					return;
				}
				else{

					var cks=$("#ck").val();
					var sta=$("#sta").val();
					if(cks===''){
						alert("请选择开通时长!");
						return;
					}
					location.href="/vrs/pay/to_pay?vid="+cks+"&sta="+sta;


				}

			})


		}
	</script>
   
   
    
