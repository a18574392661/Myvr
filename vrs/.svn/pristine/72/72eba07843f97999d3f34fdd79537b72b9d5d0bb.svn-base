* {
    margin: 0;
    padding: 0;
}

html {
    background: #343636;
}

body {
    display: none;
    font-family: 'Segoe UI', sans-serif;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

.wrap {
