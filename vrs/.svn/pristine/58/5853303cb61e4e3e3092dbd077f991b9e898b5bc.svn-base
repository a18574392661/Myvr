		<script>

			function register2() {
				var password=$("#password").val();
				var name=$("#name").val();
				var rid=$("#rid").val();
				var pwd2=$("#password3").val();
				var mobile=$("#mobile2").val();

				if (mobile==''||mobile==null){
					alert("手机号码不能为空!");
					return;
				}
				/*  if(rid===''||rid==null){
                      alert("请选择级别");
                      return ;
                  }*/
				if (password===''){
					alert("密码不能为空!")
					return;
				}

				//"name":name,
				if(pwd2!=password){
					alert("两次密码不一样!!");
					return;
				}
				$.post("/vrs/user/addH5user",{"mobile":mobile,"password":password,"rid":rid},function (data) {


					alert(data.msg);
					if(data.code==0){
						location.href="/vrs/user/login"
					}



				})


			}


			//查询
			function  toSearch() {
					var name=$("#n").val();
				    location.href="/pro/h5/quertSearchPro?name="+name;
			}

			function toPage(page) {
				 var n=$("#name").val();
				 location.href='/pro/h5/quertSearchPro?page='+page+"&name="+n;
			}

		</script>
