<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content ">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox float-e-margins">
					<div class="ibox-content">
						<form class="form-horizontal m-t" id="signupForm">
						<input id="id" name="id" th:value="${zw.id}"  type="hidden">
							<div class="form-group">	
								<label class="col-sm-3 control-label">座位编号：</label>
								<div class="col-sm-8">
									<input id="code" name="code" th:value="${zw.code}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">1 开启 0关闭：</label>
								<div class="col-sm-8">
									<input id="status" name="status" th:value="${zw.status}" class="form-control" type="text">
								</div>
							</div>
														<div class="form-group">	
								<label class="col-sm-3 control-label">对应的课程id：</label>
								<div class="col-sm-8">
									<input id="cid" name="cid" th:value="${zw.cid}" class="form-control" type="text">
								</div>
							</div>
																					<div class="form-group">
								<div class="col-sm-8 col-sm-offset-3">
									<button type="submit" class="btn btn-primary">提交</button>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/ts/zw/edit.js">
	</script>
</body>
</html>
