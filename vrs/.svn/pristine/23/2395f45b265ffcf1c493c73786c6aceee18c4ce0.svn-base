			</section>
			<section style="padding-top: 10px;">
				<div class="block no-bottom">
					<div class="container">
						 <ul style="margin-top: -10%;">
						    <!-- 图片显示 -->
							<li th:each="pros,userStat : ${listPro}">
								<div class="container">
									<div class="row">
										<div class="col-sm-4" th:each="pro,sta : ${pros}" style="margin-top: 1rem;"  >
											<span th:if="${userStat.index==0 and sta.index==0}">
											<input type="hidden" id="qjt" th:value="${pro.qjtfile}">
													<input type="hidden" id="mid" th:value="${pro.id}">
													<!--	<span style="color: #0f3e68;text-align: center; margin-top: 3rem;" th:onclick="'javascript:showProDetail('+${pro.id}+');'" >查看详情</span>-->
											<div id="pano" style="width:100%;height:100%;min-height: 300px;" onclick="ff()">
											<noscript><table style="width:100%;height:100%;"><tr style="vertical-align:middle;"><td><div style="text-align:center;">ERROR:<br/><br/>Javascript not activated<br/><br/></div></td></tr></table></noscript>
											<script>
												embedpano({swf:"/h5pano/tour.swf", xml:"/h5pano/tour.xml", target:"pano", html5:"auto", mobilescale:1.0, passQueryParameters:true});
												/*定义控件*/
												var krpano = document.getElementById("krpanoSWFObject");
												/*******************动态数据获取*******************/
														//d902bcc4927747d49739656a27b6db34.jpg
												var name = "aa3456345";
												//7bc378b8704a441496a6ff6a438dd77a.tiles
												var sd_path2 ="/files/qjt/vtour/panos/"+$("#qjt").val();
												var xml = '<scene name="'+name+'" title="1" onstart=""   lat="" lng="" heading="">' +
														'<view hlookat="0.0" vlookat="0.0" fovtype="MFOV"  fov="100" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" /><image ><cube url="'+ sd_path2 + '/pano_%s.jpg"/></image></scene>';

												//if( !krpano ) krpano = document.getElementById("krpanoSWFObject");

												krpano.call("loadxml(" + xml + ");");//切换场景
												krpano.call("skin_loadscene("+name+");");//加载某个场景

											</script>


											</div>

										</span>
										<!--	<input type="text" th:value="${sta.index}">
										th:if="${(userStat.index !=0) || (userStat.index ==0 and sta.index >0) }"
										-->
											<span th:if="${(userStat.index !=0) || (userStat.index ==0 and sta.index >0) }">
													<img th:src="${pro.simg}" th:onclick="'javascript:toPage('+${pro.id}+');'" style="width:320px;height:300px;">
											</span>

											<!--<img th:src="${pro.img}" th:onclick="'javascript:toPage('+${pro.id}+');'" style="width:300px;height:300px;">-->
										</div>
									</div>
