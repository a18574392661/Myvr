<script>

   $(function () {
   var cid=$("#cid").val();
   			//查所有
	   $.get("/vrs/houses/getClsAll",function (data) {

				   for (let i = 0; i < data.length; i++) {
					    if(data[i].id==cid){
					    	//查询父级显示出来子级别
							$.get("/vrs/houses/getClsChild",{"pid":data[i].pid},function (chi) {
								for (let i = 0; i< chi.length; i++) {
									var path='/vrs/houses/pageListG?cid='+chi[i].id;
									if (data[i].id==cid){
										$("#child").append('<a  class="butn1 butn-light" href="'+path+'">'+chi[i].name+'</a>');
									}else{
										$("#child").append('<a  class="butn1 butn-light" href="'+path+'">'+chi[i].name+'</a>');
									}


								}
							})

						}
				   }


	  		 }

   			)

   })

	/*查询子分类*/
	function  to_child(pid) {
		//alert(pid)
			$("#child").empty();
		$.get("/vrs/houses/getClsChild",{"pid":pid},function (data) {


				for (let i = 0; i < data.length; i++) {
					var path='/vrs/houses/pageListG?cid='+data[i].id;
					$("#child").append('<a  class="butn1 butn-light" href="'+path+'">'+data[i].name+'</a>');

			}
		})
	}

	function toPage(id) {
  location.href="/pro/h5/proHuance?id="+id;
	}


</script>


<script>
	function ff(){
		var id=$("#mid").val();
		location.href="/pro/h5/proHuance?id="+id;
	}

	$(function () {
		var i=$("#ste").val();
		$("#mu"+i).css("color","red");


	})
</script>
    

    
