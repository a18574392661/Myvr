						<div class="info-box">

							<!--th:onclick="'javascript:to_child('+${c.id}+');'"-->
							<a class="butn1 butn-light"  style="color: #0d8ddb"  th:onclick="'javascript:to_child('+${c.id}+');'"  th:each="c,userStat : ${category}"  th:text="${c.name}"></a>
