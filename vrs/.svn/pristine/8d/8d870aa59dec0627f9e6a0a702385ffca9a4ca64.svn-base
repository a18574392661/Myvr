			function to_det(){
				var id=$("#mid").val();

				var len=$("input[name='cids']:checked").length;
				var str="";

				$("input[name='cids']:checked").each(function(i){//把所有被选中的复选框的值存入数组
					str+=this.value;
					if(len-1>i){
						str+=",";

					}


				});
				location.href="/pro/h5/proDetailed?id="+id+"&cids="+str;
			}

function showProDetail(id) {
	var len=$("input[name='cids']:checked").length;
	var str="";

	$("input[name='cids']:checked").each(function(i){//把所有被选中的复选框的值存入数组
		str+=this.value;
		if(len-1>i){
			str+=",";

		}


	});


	location.href="/pro/h5/proDetailed?cids="+str+"&id="+id;

}





