			function ff(id,index) {


			//	alert(index)
				//	alert("aaa"+id+"//"+index);
				//当前行的其他标签字体复原
			//	debugger;

					//var shang=$("#"+index+"shang");//
				var shang=$("#shang"+index);
				//alert(shang.children("span").length)
				var f = shang.children("span");

				for(var i = 0;i < f.length;i++	){
					$(shang.children("span")[i]).children("font").css("color","#9D9D9D")


					$(shang.children("span")[i]).children("input").attr("checked",false)
				}



